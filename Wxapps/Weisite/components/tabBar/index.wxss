/* components/tabBar/index.wxss */
.tabBar-wrap {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    display: flex;
    background: #fff;
    z-index: 999;
    border-top: 1rpx solid #ccc;
    padding: 10rpx 0;
}
.tabBar-wrap view {
    flex: 1;
    text-align: center;
    font-size: 24rpx;
}
.tabBar-wrap view image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
}